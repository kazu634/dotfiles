[color]
  # colorの設定(以下のコマンドは自動で色つける)
  status = auto
  diff = auto
  branch = auto
  interactive = auto
  grep = auto
[core]
  excludesfile = ~/.gitignore  # globalな.gitignoreの指定
  editor = /usr/bin/vim
[alias]
  # log関連
  logg = log --stat --pretty=format:'%Cblue%h %Cgreen%ar %Cred%an %Creset%s %Cred%d'
  # log + graph
  graph = log --graph --date-order --pretty=format:'%Cblue%h %Cgreen%ci %Cred%an %Cblue%m %Creset%s %Cred%d'
  # status
  st = status -s -b
